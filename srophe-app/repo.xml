    <copyright>true</copyright>
    <type>application</type>
    <target>geo</target>
    <prepare>pre-install.xql</prepare>
    <finish>post-install.xql</finish>
    <deployed>2016-12-05T10:24:28.879-05:00</deployed>
    <config>
        <!-- App title -->
        <title>GEO</title>
        <!-- App description -->
        <description>GEO</description>
        <!-- Contact email -->
        <contact>info@syriaca.org</contact>
        <!-- Web Site URL -->
        <url>http://syriaca.org/</url>
        <!-- Root of app for building dynamic links. Default is eXist app root -->
        <nav-base>/exist/apps/geo</nav-base>
        <!-- eXist app root for app deployment-->
        <app-root>geo</app-root>
        <!-- eXist data app root for gazetteer data -->
        <data-root>srophe-data</data-root>
        <!-- Base URI for identifiers in app data -->
        <base_uri>http://syriaca.org</base_uri>
        <!-- Map rendering -->
        <maps>
            <option>google</option>
            <option selected="true">leaflet</option>
        </maps>
        <keyboard-options>
            <ul xmlns="http://www.w3.org/1999/xhtml" class="dropdown-menu">
                <li id="syriac-phonetic">Syriac Phonetic</li>                                
                <li id="syriac-standard">Syriac Standard</li>
                <li id="ms-Arabic (101)">Arabic Mod. Standard</li>
                <li id="qwerty">English QWERTY</li>                                    
                <li id="ms-Greek">Greek Mod. Standard</li>
                <li id="ms-Russian">Russian Mod. Standard</li>
            </ul>
        </keyboard-options>
